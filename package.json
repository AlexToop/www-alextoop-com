{
  "name": "vue-webpack-multi-page-template",
  "version": "1.0.0",
  "description": "A vue webpack multi-page template",
  "author": "Alexander Toop",
  "scripts": {
    "clean": "rm -rf build",
    "start": "npm run clean && npm run build && NODE_ENV=development webpack-dev-server --hot --open",
    "build": "npm run clean && NODE_ENV=production webpack --config webpack.config.js"
  },
  "main": "src/index.html",
  "devDependencies": {
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "bulma": "^0.8.0",
    "jquery": "^3.4.1",
    "vue": "^2.6.11"
  },
  "license": "Apache-2.0"
}
