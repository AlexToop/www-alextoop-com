{
  "name": "vue-webpack-multi-page-template",
  "version": "1.0.0",
  "description": "A vue webpack multi-page template",
  "author": "Alexander Toop",
  "scripts": {
    "clean": "rm -rf build",
    "start": "npm run clean && npm run build && NODE_ENV=development webpack-dev-server --hot --open",
    "build": "npm run clean && NODE_ENV=production webpack --config webpack.config.js",
    "lint": "eslint --ignore-path .gitignore --ext js,vue --fix .",
    "predeploy": "npm run build && aws s3 rm s3://www.alextoop.com/ --recursive",
    "deploy": "aws s3 sync ./build/ s3://www.alextoop.com/ --profile defaultMFA",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id E3D1H6J3TO2QO1 --paths \"/*\""
  },
  "main": "src/index.html",
  "devDependencies": {
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "aws-cdk": "^1.72.0",
    "bulma": "^0.8.0",
    "jquery": "^3.5.0",
    "vue": "^2.6.11"
  },
  "license": "Apache-2.0"
}
